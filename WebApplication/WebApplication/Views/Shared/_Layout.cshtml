<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>@ViewBag.Title - My ASP.NET Application</title>
        <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
        <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
        <script src="https://kit.fontawesome.com/409314b6dc.js" crossorigin="anonymous"></script>
    </head>
    <body>
        <div class="navbar navbar-dark bg-dark mb-3">
            <div class="container">
                <div class="navbar-header">
                    @Html.ActionLink("Marcas", "Index", "Marca", new { area = "" }, new { @class = "navbar-brand" })
                    @Html.ActionLink("Sucursales", "Index", "Sucursal", new { area = "" }, new { @class = "navbar-brand" })
                    @Html.ActionLink("Clientes", "Index", "Clientes", new { area = "" }, new { @class = "navbar-brand" })
                    @Html.ActionLink("Empleados", "Index", "Empleado", new { area = "" }, new { @class = "navbar-brand" })
                    @Html.ActionLink("Buses", "Index", "Bus", new { area = "" }, new { @class = "navbar-brand" })
                    @Html.ActionLink("Viajes", "Index", "Viaje", new { area = "" }, new { @class = "navbar-brand" })
                </div>
            </div>
        </div>

        <div class="container body-content">
            @RenderBody()
            
            <!--<footer>
            @*<p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>*@
            </footer>-->
        </div>

        <script src="~/Scripts/jquery-3.6.0.min.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
    </body>
</html>